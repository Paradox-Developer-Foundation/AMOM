types = {
	type[country_history] = {
		path = "game/history/countries"
		type_per_file = yes
	}
}

country_history = {
	capital = int

	## cardinality = 0..1
	oob = <oob>
	alias_name[effect] = alias_match_left[effect]
	
	### adds character to country
	## cardinality = 0..inf
	recruit_character = <character>
	### adds character to country
	## cardinality = 0..inf
	recruit_character = value[event_target]
	### adds character to country
	## cardinality = 0..inf
	recruit_character = value[global_event_target]
	### adds character to country
	## cardinality = 0..inf
	recruit_character = scope[character]

	### Set country's current train buffer
	## cardinality = 0..inf
	starting_train_buffer = float

	## cardinality = 0..inf
	date_field = {

		## cardinality = 0..1
		capital = <state> 
		
		## cardinality = 0..inf
		oob = <oob>
		alias_name[effect] = alias_match_left[effect]

		### adds character to country
		## cardinality = 0..inf
		recruit_character = <character>
		### adds character to country
		## cardinality = 0..inf
		recruit_character = value[event_target]
		### adds character to country
		## cardinality = 0..inf
		recruit_character = value[global_event_target]
		### adds character to country
		## cardinality = 0..inf
		recruit_character = scope[character]
	}
}
